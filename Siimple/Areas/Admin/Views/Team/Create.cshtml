@model CreateTeamVm
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="exampleFormControlInput1" asp-for="Bio">Bio</label>
        <input type="text" class="form-control" asp-for="Bio">
        <span class="text-danger" asp-validation-for="Bio"></span>
    </div>
    <div class="form-group">
        <label for="exampleFormControlInput1" asp-for="Image">Image</label>
        <input class="form-control" asp-for="Image">
        <span class="text-danger" asp-validation-for="Image"></span>
    </div>
    <div class="form-group">
        <label for="exampleFormControlInput1" asp-for="Iconname">Icon</label>
        <input class="form-control" type="text" asp-for="Iconname">
        <span class="text-danger" asp-validation-for="Iconname"></span>
    </div>
    <div class="form-group">
        <label for="exampleFormControlSelect2" asp-for="TitleId">Title</label>
        <select class="custom-select" asp-for="TitleId">
            @foreach (Title item in (List<Title>)ViewData["Title"])
            {
                <option value="@item.Id">@item.Name</option>

            }
        </select>
    </div>
    <button type="submit" class="btn btn-outline-success">Create</button>
</form>